        [featureKey]: { ...prev[featureKey], enabled }
      }));
      
      toast.success(\`Feature \${enabled ? 'enabled' : 'disabled'}\`);
    } catch (error) {
      toast.error('Failed to update feature flag');
    }
  };

  const updateRollout = async (featureKey: string, rollout: number) => {
    try {
      await updateFeatureRollout(featureKey, rollout);
      setFeatures(prev => ({
        ...prev,
        [featureKey]: { ...prev[featureKey], rollout }
      }));
    } catch (error) {
      toast.error('Failed to update rollout percentage');
    }
  };

  return (
    <div className="space-y-6">
      <div className="flex items-center justify-between">
        <h2 className="text-2xl font-bold">Feature Toggles</h2>
        <Badge variant="info">Admin Panel</Badge>
      </div>
      
      {Object.entries(features).map(([key, feature]) => (
        <Card key={key} variant="modern">
          <CardContent className="p-6">
            <div className="flex items-center justify-between mb-4">
              <div>
                <h3 className="font-semibold capitalize">
                  {key.replace(/([A-Z])/g, ' $1').trim()}
                </h3>
                <p className="text-sm text-gray-600">
                  {getFeatureDescription(key)}
                </p>
              </div>
              
              <Switch
                checked={feature.enabled}
                onCheckedChange={(checked) => toggleFeature(key, checked)}
                variant="premium"
                size="lg"
              />
            </div>
            
            {feature.enabled && (
              <div className="space-y-3">
                <div className="flex items-center justify-between">
                  <span className="text-sm font-medium">Rollout Percentage</span>
                  <span className="text-sm text-gray-600">{feature.rollout}%</span>
                </div>
                
                <Slider
                  value={feature.rollout}
                  onValueChange={(value) => updateRollout(key, value)}
                  min={0}
                  max={100}
                  step={5}
                  variant="gradient"
                />
                
                <div className="flex justify-between text-xs text-gray-500">
                  <span>0% (Disabled)</span>
                  <span>100% (All Users)</span>
                </div>
              </div>
            )}
          </CardContent>
        </Card>
      ))}
    </div>
  );
};`
      }
    ]
  };

  return examples[componentId] || [];
};

